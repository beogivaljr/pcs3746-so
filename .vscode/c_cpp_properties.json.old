{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/1/linux/include",
                "${workspaceFolder}/1/linux/include/uapi",
                "${workspaceFolder}/1/linux/include/generated",
                "${workspaceFolder}/1/linux/arch/arm/include/**"
            ],
            "defines": [
                "__GNUC__",
                "__KERNEL__"
            ],
            "intelliSenseMode": "gcc-arm",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/1/linux/include",
                    "${workspaceFolder}/1/linux/mm",
                    "${workspaceFolder}/1/linux/fs",
                    "${workspaceFolder}/1/linux/kernel"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "compilerPath": "/usr/bin/arm-linux-gnueabi-gcc",
            "cStandard": "c89",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}